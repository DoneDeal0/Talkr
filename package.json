{
  "name": "talkr",
  "version": "3.4.0",
  "description": "Talkr is the lightest i18n provider for React applications. It supports Typescript, provides autocompletion, has 0 dependencies, and is very easy to use.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "declaration": true,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && mkdirp dist && rollup -c",
    "test": "jest",
    "lint": "eslint",
    "tsc": "tsc --noEmit --incremental",
    "format": "npx prettier . --write",
    "publish": "npm publish --access=public",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DoneDeal0/Talkr"
  },
  "keywords": [
    "i18n",
    "react",
    "react-native",
    "react native",
    "reactjs",
    "localization",
    "globalization",
    "translations",
    "translation",
    "internationalization",
    "l10n",
    "talkr"
  ],
  "author": "DoneDeal0",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@eslint/js": "^9.9.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "babel-jest": "^29.3.1",
    "eslint": "^9.9.0",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "mkdirp": "^1.0.4",
    "prettier": "^3.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.76.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3",
    "typescript-eslint": "^8.2.0"
  }
}
